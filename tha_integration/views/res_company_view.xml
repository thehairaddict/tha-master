<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="base_view_company_form_inherit" model="ir.ui.view">
            <field name="name">base.res.company.form.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="The Hair Addict">
                        <group>
                            <group>
                                <field name="tha_url"/>
                                <field name="last_order_created" invisible="0"/>
                            </group>
                            <group>
                                <field name="consumer_key" password="True"/>
                                <field name="consumer_secret" password="True"/>
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>